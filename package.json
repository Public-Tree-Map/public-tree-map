{
  "name": "tree-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "leaflet": "^1.3.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-leaflet": "^2.0.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
